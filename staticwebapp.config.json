{
    "routes": [
      {
        "route": "/*",
        "serve": "/index.html",
        "statusCode": 200
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html"
    },
    "responseOverrides": {
        "400": {
          "rewrite": "/index.html"
        },
        "401": {
          "redirect": "/login",
          "statusCode": 302
        },
        "403": {
          "rewrite": "/index.html"
        },
        "404": {
          "rewrite": "/index.html"
        }
    }
}