{
  "name": "fnr",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/data-grid": "^4.0.0-alpha.37",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.3.10",
    "axios": "^0.21.4",
    "bootstrap": "^5.1.3",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "*",
    "tailwindcss": "^2.2.16",
    "tailwindcss-dir": "^4.0.0",
    "web-vitals": "^1.0.1",
    "xlsx": "^0.17.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10"
  },
  "scripts": {
    "start": "cross-env GENERATE_SOURCEMAP=true env-cmd -f .env.development react-scripts start",
    "build": "cross-env GENERATE_SOURCEMAP=false env-cmd -f .env.test react-scripts build",
    "build:production": "cross-env GENERATE_SOURCEMAP=false env-cmd -f .env.production react-scripts build",
    "build:test": "cross-env GENERATE_SOURCEMAP=false env-cmd -f .env.test react-scripts build",
    "build:uat": "cross-env GENERATE_SOURCEMAP=false env-cmd -f .env.uat react-scripts build",
    "tailwind": "tailwind build ./src/styles/tailwind-config.css -c ./tailwind.config.js -o ./src/styles/tailwind.css",
    "purge-tailwind": "node ./purge-tailwindcss.js",
    "test": "react-scripts test --env=node",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "/"
}
